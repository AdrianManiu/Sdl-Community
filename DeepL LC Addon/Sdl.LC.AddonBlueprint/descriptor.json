{
  "Descriptor": {
    "name": "DeepL Translation Provider",
    "version": "1.0.0",
    "description": "This Add on provides machine translation results from the DeepL Translator.",
    "extensions": [
      {
        "id": "FIRST_AUTO_TASK_TYPE",
        "extensionPointId": "AUTO_TASK_TYPE",
        "name": "Fist Auto Task Type",
        "description": "Provides some automated task types.",
        "extensionPointVersion": "1",
        "configuration": {
          "endpoints": {
            "lcautomatictasksubmit": "submit"
          },
          "scope": "file",
          "supportedInputFileType": "nativeSource",
          "outputFileType": "bcmSource",
          "outcomes": [
            {
              "outcome": "outcome1",
              "description": "The first outcome of the taskType",
              "default": false
            },
            {
              "outcome": "outcome2",
              "description": "The second outcone of the taskType",
              "default": true
            }
          ],
          "workflowTemplateConfigurations": [
            {
              "name": "Sample Configuration 1",
              "id": "SAMPLE_1",
              "description": "Sample description",
              "optional": false,
              "defaultValue": "null",
              "dataType": "string"
            },
            {
              "name": "Sample Configuration 2",
              "id": "SAMPLE_2",
              "description": "Sample description",
              "optional": false,
              "defaultValue": "null",
              "dataType": "number"
            }
          ]
        }
      }
    ],
    "baseUrl": "https://localhost:44373",
    "standardEndpoints": {
      "health": "/api/health",
      "addonLifecycle": "/api/addon-lifecycle",
      "accountLifecycle": "/api/account-lifecycle",
      "documentation": "/api/help",
      "configuration": "/api/configuration"
    },
    "scopes": [],
    "configurations": [
      {
        "name": "API Key",
        "id": "apiKey",
        "description": "Please add DeepL Api Key",
        "optional": false,
        "defaultValue": "null",
        "dataType": "secret"
