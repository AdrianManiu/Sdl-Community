{
	"Descriptor": {
		"name": "DeepL Translation Provider",
		"version": "1.0.6",
		"description": "This Add on provides machine translation results from the DeepL Translator.",
		"extensions": [
			{
				"id": "MT_DeepL",
				"extensionPointId": "lc.mtprovider",
				"name": "DeepL Translation Provider",
				"description": "rovides translation using DeepL",
				"extensionPointVersion": "1",
				"configuration": {
					"endpoints": {
						"sdl.lc.extensionpoint.machinetranslationprovider.translate": "api/translate",
						"sdl.lc.extensionpoint.machinetranslationprovider.engines": "api/translation-engines"
					},
					"format": "html"
				}
			}
		],
		"baseUrl": "http://192.168.68.113:2509",
		"standardEndpoints": {
			"health": "/api/health",
			"addonLifecycle": "/api/addon-lifecycle",
			"accountLifecycle": "/api/account-lifecycle",
			"documentation": "/api/help",
			"configuration": "/api/configuration"
		},
		"scopes": [],
		"configurations": [
			{
				"name": "API Key",
				"id": "apiKey",
				"description": "Please add DeepL Api Key",
				"optional": false,
				"defaultValue": null,
				"dataType": "secret"
			},
			{
				"name": "Translation formality",
				"id": "translationFormality",
				"description": "Please choose if translated text should lean towards formal or informal language. Possible options are 'Default','More','Less'",
				"optional": true,
				"defaultValue": null,
				"dataType": "string"
			}
		],
		"releaseNotes": "incididunt",
		"minimumVersion": "1.0.0",
		"vendor": {
			"name": "SDL Community",
			"url": "acme.com"
		}
	}
}